<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muthoyapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MuthoyApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".test"
            android:exported="false" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".ScratchWin1Activity" />
        <activity android:name=".MathQuiz3Activity" />
        <activity
            android:name=".MathQuiz2Activity"
            android:parentActivityName=".MathQuizActivity"
            android:label="Math Quiz"/>
        <activity
            android:name=".MathQuizActivity"
            android:parentActivityName=".RewardsActivity"
            android:label="Math Quiz"/>
        <activity android:name=".RewardsHistory1Activity" />
        <activity
            android:name=".RewardsHistoryActivity"
            android:parentActivityName=".RewardsActivity"
            android:label="Rewards History"/>
        <activity android:name=".WinActivity" />
        <activity
            android:name=".ScratchWinActivity"
            android:parentActivityName=".RewardsActivity"
            android:label="Scratch and Win"/>
        <activity
            android:name=".RedeemActivity"
            android:parentActivityName=".RewardsActivity"
            android:label="Reedem" />
        <activity android:name=".RewardsActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".VerificationCodeActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".SpinWinActivity"
            android:parentActivityName=".RewardsActivity"
            android:label="Spin and Win"/>
        <activity android:name=".SigninActivity" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>